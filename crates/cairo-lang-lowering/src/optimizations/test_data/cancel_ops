//! > ==========================================================================

//! > Test destructure after merge.

//! > test_runner_name
test_cancel_ops

//! > function
fn foo(a: felt252) -> felt252 {
   let tuple = match a {
      0 => (1, (2, 3)),
      _ => {
        let tuple = (a, (2, 3));
        use_tuple(tuple);
        tuple
      },
   };
   let (b, (c, d)) = tuple;
   b + c + d
}

//! > function_name
foo

//! > module_code
extern fn use_tuple<T>(a: (felt252, T)) nopanic;

//! > semantic_diagnostics

//! > lowering_diagnostics

